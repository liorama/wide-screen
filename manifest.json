{
  "name": "Width Maximizer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Makes the Github diff screen utilize the full width of the screen",
  "homepage_url": "http://google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "bin/css/inject.css"
      ]
    }
  ]
}